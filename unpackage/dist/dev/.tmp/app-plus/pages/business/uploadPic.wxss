}
.uni-tip-title {
   	text-align: center;
   	font-weight: bold;
   	font-size: 32rpx;
   	color: #333;
}
.uni-tip-content {
   	padding: 30rpx;
   	font-size: 28rpx;
   	color: #666;
	text-align: center;
}
.uni-tip-group-button {
   	margin-top: 20rpx;
   	display: -webkit-box;
   	display: -webkit-flex;
   	display: -ms-flexbox;
